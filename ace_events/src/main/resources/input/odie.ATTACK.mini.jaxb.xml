<odie_input>
  <predicate>
    <name>Attack</name>
  </predicate>
  <seeds>
    <seed>attack</seed>
    <seed>clash</seed>
    <seed>conflict</seed>
    <seed>shoot</seed>
    <seed>bomb</seed>
    <seed>explode</seed>
    <seed>battle</seed>
  </seeds>
  <arguments>
    <argument>
      <role>Attacker</role>
      <types>person, organization, geopolitical entity</types>
      <examples>
        <example>forces</example>
        <example>Israel</example>
        <example>terrorist</example>
        <example>man</example>
      </examples>
    </argument>
    <argument>
      <role>Target</role>
      <types>person, organization, vehicle, facility, weapon</types>
      <examples>
        <example>Palestinian</example>
        <example>Fallujah</example>
        <example>soldier</example>
        <example>café</example>
        <example>car</example>
      </examples>
    </argument>
    <argument>
      <role>Instrument</role>
      <types>weapon,vehicle</types>
      <examples>
        <example>bomb</example>
        <example>rifle</example>
        <example>gun</example>
        <example>stone</example>
      </examples>
    </argument>
    <argument>
      <role>Time</role>
      <types>time</types>
      <examples>
        <example>Sunday night</example>
        <example>Thursday</example>
        <example>45-minute</example>
      </examples>
    </argument>
    <argument>
      <role>Place</role>
      <types>geopolitical entity,location,facility</types>
      <examples>
        <example>Baghdad</example>
        <example>Yemen</example>
        <example>market</example>
        <example>Jerusalem</example>
        <example>café</example>
      </examples>
    </argument>
  </arguments>
  <usage_samples>
    <sample>Terrorists clashed soldiers in Baghdad on Thursday.</sample>
    <sample>Men shot at the the car with rifles in Yemen on Sunday night.</sample>
    <sample>forces had a 45-Minute gun battle with Palestinians at the market.</sample>
  </usage_samples>
  <global_context>
    <phrases>war,killing</phrases>
  </global_context>
</odie_input>